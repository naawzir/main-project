        "eslint": "^4.11.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-mocha": "^4.11.0",
        "eslint-plugin-react": "^7.11.1",
        "laravel-mix": "^4.0.0",
        "mocha": "^4.0.1",
        "mocha-junit-reporter": "^1.18.0",
        "nightmare": "^3.0.1",
        "popper.js": "^1.14.6",
